{
  "name": "Piratechest",
  "description": "Safer magnet management",
  "version": "0.4.5",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Piratechest",
      "default_icon": "icon.png",
      "default_popup": "menu.html"
  },
  "background": {
      "page": "index.html"
  },
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "include_globs": [
        "*google.*"
      ],
      "css": ["piratechest.css"],
      "js": ["r.js"]
    }],
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
